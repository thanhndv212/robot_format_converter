<?xml version="1.0" ?>
<robot name="example_robot">
  <link name="base_link">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0.0 0.1" rpy="0 0 0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.05" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>
  <link name="link1">
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0.0 0.0 0.1" rpy="0 0 0"/>
      <inertia ixx="0.01" iyy="0.01" izz="0.005" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>
  <link name="link2">
    <inertial>
      <mass value="0.5"/>
      <origin xyz="0.1 0.0 0.0" rpy="0 0 0"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.001" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>
  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0.0 0.0 0.2" rpy="0 0 0"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit lower="-3.14159" upper="3.14159" effort="100.0" velocity="2.0"/>
  </joint>
  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0.0 0.0 0.2" rpy="0 0 0"/>
    <axis xyz="1.0 0.0 0.0"/>
    <limit lower="-1.57" upper="1.57" effort="50.0" velocity="1.5"/>
  </joint>
</robot>
